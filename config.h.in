/* silly types */
#undef HAVE_INT8_T
#undef HAVE_UINT8_T
#undef HAVE_INT16_T
#undef HAVE_UINT16_T
#undef HAVE_INT32_T
#undef HAVE_UINT32_T

/* definitions for silly archs */
#ifndef HAVE_INT8_T
#define HAVE_INT8_T 1
typedef char int8_t;
#endif
#ifndef HAVE_UINT8_T
#define HAVE_UINT8_T 1
typedef unsigned char uint8_t;
#endif
#ifndef HAVE_INT16_T
#define HAVE_INT16_T 1
typedef short int16_t;
#endif
#ifndef HAVE_UINT16_T
#define HAVE_UINT16_T 1
typedef unsigned short uint16_t;
#endif
#ifndef HAVE_INT32_T
#define HAVE_INT32_T 1
typedef int int32_t;
#endif
#ifndef HAVE_UINT32_T
#define HAVE_UINT32_T 1
typedef unsigned int uint32_t;
#endif
